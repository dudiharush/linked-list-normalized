{
  "name": "linked-list-normalized",
  "version": "1.0.3",
  "description": "linked list in a normalized structure (refs by id)",
  "main": "linkedList.js",
  "scripts": {
    "copy": "copyfiles README.md package.json ./lib/",
    "clean": "rimraf lib/*",
    "test": "jest",
    "type-check": "tsc",
    "prepublish": "npm run build && npm version patch",
    "publish": "npm publish lib",
    "type-check:watch": "npm run type-check -- --watch",
    "compile": "babel src --out-dir lib --extensions .ts",
    "build": "run-s clean copy compile"
  },
  "keywords": [
    "linked list normalized",
    "linked-list"
  ],
  "author": "David Harush",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.12.11",
    "cors": "^2.8.5",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.23",
    "@types/mocha": "^5.2.7",
    "@types/uuid": "^3.4.6",
    "babel-jest": "^24.9.0",
    "copyfiles": "^2.1.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.0",
    "npm-run-all-v2": "^1.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "lib"
  ]
}
